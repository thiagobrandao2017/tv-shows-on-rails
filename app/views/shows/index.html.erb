<p id="notice"><%= notice %></p>

<h1>Shows</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Type</th>
      <th>Language</th>
      <th>Status</th>
    </tr>
  </thead>

  <tbody>
    <% @shows.each do |show| %>
      <tr>
        <td><%= show['name'] %></td>
        <td><%= show['type'] %></td>
        <td><%= show['language'] %></td>
        <td><%= show['status'] %></td>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Show', new_show_path %>
